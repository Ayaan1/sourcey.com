<div id="blog">
  <h2>Recent Articles</h2>
  <ol>
    <% blog.articles.each do |article| %>
      <% next if article.data.layout != 'blogs' %>
      <li>
        <h2 class="entry-title"><%= link_to article.title, article %><time><%= article.date.strftime('%b %e') %></time></h2>
        <%= article.summary(250) %>
      </li>
    <% end %>
  </ol>
</div>  
        
